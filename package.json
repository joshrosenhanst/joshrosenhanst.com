{
  "name": "joshrosenhanst.com",
  "version": "1.0.1",
  "description": "Personal site: [joshrosenhanst.com](https://joshrosenhanst.com)",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "sass src/sass/index.sass dist/css/index.css",
    "css_production": "sass src/sass/index.sass dist/css/index.css --style compressed",
    "html": "pug src/templates --out dist",
    "assets": "cp -R src/assets dist",
    "build": "npm run html && npm run css && npm run assets",
    "production": "npm run html && npm run css_production && npm run assets",
    "prod": "npm run production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshrosenhanst/joshrosenhanst.com.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshrosenhanst/joshrosenhanst.com/issues"
  },
  "homepage": "https://github.com/joshrosenhanst/joshrosenhanst.com#readme",
  "dependencies": {
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.17.3"
  }
}
